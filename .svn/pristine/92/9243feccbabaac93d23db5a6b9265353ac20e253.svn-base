@charset "UTF-8";
/* CSS Document */
@font-face {
  font-family: 'robotothin';
  src: url('/node_modules/roboto-fontface/fonts/Roboto-Thin.eot');
  src: url('/node_modules/roboto-fontface/fonts/Roboto-Thin.eot?#iefix') format('embedded-opentype'),
  url('/node_modules/roboto-fontface/fonts/Roboto-Thin.woff2') format('woff2'),
  url('/node_modules/roboto-fontface/fonts/Roboto-Thin.woff') format('woff'),
  url('/node_modules/roboto-fontface/fonts/Roboto-Thin.ttf') format('truetype'),
  url('/node_modules/roboto-fontface/fonts/Roboto-Thin.svg#robotothin') format('svg');
  font-weight: normal;
  font-style: normal;
}

