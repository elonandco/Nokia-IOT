<div class="wf-data-event step-content">
	<div class="spinner-container">
	  <div class="progress-window page-loader">
	    <div class="window">
	      <h2 *ngIf="content?.header" >{{content.header}}</h2>
	      <h2 *ngIf="!content?.header" >{{header}}</h2>
	      <div class="lg spinner"></div>
	      <div class="{{iconClass}}"></div>
	      <h4 *ngIf="content?.body" >{{content.body}}</h4>
	      <h4 *ngIf="!content?.body" >{{message}}</h4>
	    </div>
	  </div>
	</div>
</div>


