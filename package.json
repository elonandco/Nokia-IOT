    "docs": "./node_modules/typedoc/bin/typedoc --options typedoc.json src/**/*.ts",
    "build-css": "nodemon -e scss -x \"node-sass src -o styles\"",
    "dist": "gulp",
    "clean": "gulp clean",
    "ci-build": "gulp dist"
  },
